{{!< default}} <main id="content" class="container content-inner" role="main">
    <section class="error-content error-404 js-error-container">
        <section class="error-details">
            <section class="error-title">
                <h1 class="error-code">{{code}}</h1>
                <p class="error-description">{{message}}</p>
                <br /> <br />
                <a class="error-link" href="{{@blog.url}}">Go to the home page â†’</a>
            </section>
        </section>
    </section>

    {{#if stack}}
    <section class="error-stack">
        <h3>Stack Trace</h3>
        <p><strong>{{message}}</strong></p>
        <ul class="error-stack-list">
            {{#each stack as |item|}}
            <li>
                at
                {{#if item.function}}<em class="error-stack-function">{{item.function}}</em>{{/if}}
                <span class="error-stack-file">({{item.at}})</span>
            </li>
            {{/each}}
        </ul>
    </section>
    {{/if}}
    </main>
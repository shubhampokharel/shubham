<header>
	<div class="header-container">
		<div class="container">
			<a href="javascript:void(0)" class="nav-trigger" onclick="myFunction()" title="{{t " Menu"}}"><i></i></a>
			<a class="blog-logo" href="{{@site.url}}" title="{{@site.title}}">
				{{#if @site.logo}}
				<img src="{{@site.logo}}" alt="{{@site.title}}" />
				{{else}}
				{{@site.title}}
				{{/if}}
			</a>
			<div class="header-social">
				<a href="https://www.linkedin.com/in/shubhampokharel/" class="linkedin"
					title="Shubham Pokharel's Linkedin" target="_blank" rel="noopener noreferrer"><i
						class="fab fa-linkedin"></i></a>
			</div>

		</div>
	</div>

	<div class="menu-container" id="nav-container">
		<div class="menu-content">
			<div class="menu-inner">
				<div class="container">
					{{navigation}}
				</div>
			</div>
		</div>
	</div>


</header>